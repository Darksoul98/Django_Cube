[flake8]
ignore = 
    #2 blank spaces before class
    E302,
    #Line too long
    E501,
    #blank line contains whitespace
    W293,
    #trailing whitespace
    W291,
    # import be undefined, or defined from star imports: database.modelsflake8(F405)
    F405,
    #indentation contains tabsflake8(W191)
    W191,
    